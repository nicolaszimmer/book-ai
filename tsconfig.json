{
    "compilerOptions": {
      "target": "es2021",
    "lib": ["es2021"],
    "module": "node16",
    "types": ["node"],
      "declaration": true,
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "isolatedModules": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "**/__tests__/*"]
  }
  